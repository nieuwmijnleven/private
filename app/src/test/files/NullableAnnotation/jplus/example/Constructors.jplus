package jplus.example;
import org.jspecify.annotations.Nullable;

public class Constructors {
    String name;
    int age;

    Constructors (boolean flag) {
        if (flag) {
            this.name = "jadex";
        }
    }

    Constructors (boolean flag, String name) {
        if (flag) {
            this.name = name;
        }
    }

    Constructors (int age) {
        this.age = age;
    }

    Constructors (String name, int age) {
        this.name = name;
        this.age = age;
    }
}